{
    "name": "kainiklas/filament-inline-gauge",
    "description": "Inline Table Gauge Chart for FilamentPHP",
    "keywords": [
        "kainiklas",
        "laravel",
        "filament-inline-gauge",
        "filament",
        "filamentphp",
        "table",
        "column",
        "inline",
        "gauge",
        "chart"
    ],
    "homepage": "https://github.com/kainiklas/filament-inline-gauge",
    "support": {
        "issues": "https://github.com/kainiklas/filament-inline-gauge/issues",
        "source": "https://github.com/kainiklas/filament-inline-gauge"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kai",
            "email": "kai.niklas@web.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.16.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.9",
        "larastan/larastan": "^2.1",
        "orchestra/testbench": "^8.10",
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Kainiklas\\FilamentInlineGauge\\": "src/",
            "Kainiklas\\FilamentInlineGauge\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kainiklas\\FilamentInlineGauge\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kainiklas\\FilamentInlineGauge\\FilamentInlineGaugeServiceProvider"
            ],
            "aliases": {
                "FilamentInlineGauge": "Kainiklas\\FilamentInlineGauge\\Facades\\FilamentInlineGauge"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
